<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.duyi.dao.FilmDao">
    <select id="getList" resultType="com.duyi.entity.Film">
        select id,film_id,`name`,director,player,`type`,country,`length`,synopsis,play_time,img_path
        from film
    </select>

    <select id="findFilmById" resultType="com.duyi.entity.Film">
        select id,film_id,`name`,director,player,`type`,country,`length`,synopsis,play_time,img_path
        from film where film_id = #{filmId}
    </select>
</mapper>